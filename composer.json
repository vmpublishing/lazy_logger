{
    "name": "vmpublishing/lazy_logger",
    "description": "logging class proxy, that delays the instantiation to the actual logger, until there is something to log. using psr-3 and psr-11",
    "version": "0.0.1",
    "keywords": [
        "logging",
        "performance",
        "psr-3"
    ],
    "homepage": "https://github.com/vmpublishing/lazy_logger",
    "license": "MIT-like, read the LICENSE file",
    "autoload": {
        "psr-4": {
            "VM\\": "src/"
        }
    },
    "require": {
        "psr/log": "*@stable",
        "psr/container": "*@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "*@stable"
    }
}

